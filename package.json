{
  "name": "vibe-coding",
  "displayName": "Vibe Coding - VICO",
  "description": "Your AI Coding Companion - 404 Stress Not Found!",
  "version": "1.4.9",
  "publisher": "PacarAI",
  "repository": {
    "type": "git",
    "url": "https://github.com/asepindrak/vibe-coding-extension.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "media/icon.png",
  "categories": [
    "AI",
    "Chat"
  ],
  "keywords": [
    "ai",
    "chat",
    "companion",
    "companionship",
    "autocomplete",
    "code analysis",
    "code completion",
    "code explanation",
    "code recommendation",
    "coding assistant",
    "gpt",
    "chatgpt",
    "java",
    "javascript",
    "node",
    "node.js",
    "openai",
    "python",
    "refactor",
    "select code",
    "typescript"
  ],
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "js"
        ],
        "extensions": [
          ".js",
          ".jsx"
        ]
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "ts"
        ],
        "extensions": [
          ".ts",
          ".tsx"
        ]
      },
      {
        "id": "python",
        "aliases": [
          "Python",
          "py"
        ],
        "extensions": [
          ".py"
        ]
      },
      {
        "id": "java",
        "aliases": [
          "Java"
        ],
        "extensions": [
          ".java"
        ]
      },
      {
        "id": "go",
        "aliases": [
          "Go",
          "golang"
        ],
        "extensions": [
          ".go"
        ]
      },
      {
        "id": "rust",
        "aliases": [
          "Rust"
        ],
        "extensions": [
          ".rs"
        ]
      },
      {
        "id": "cpp",
        "aliases": [
          "C++"
        ],
        "extensions": [
          ".cpp",
          ".h"
        ]
      },
      {
        "id": "csharp",
        "aliases": [
          "C#",
          "csharp"
        ],
        "extensions": [
          ".cs"
        ]
      },
      {
        "id": "php",
        "aliases": [
          "PHP"
        ],
        "extensions": [
          ".php"
        ]
      },
      {
        "id": "json",
        "aliases": [
          "JSON"
        ],
        "extensions": [
          ".json"
        ]
      },
      {
        "id": "html",
        "aliases": [
          "HTML"
        ],
        "extensions": [
          ".html"
        ]
      },
      {
        "id": "css",
        "aliases": [
          "CSS"
        ],
        "extensions": [
          ".css"
        ]
      },
      {
        "id": "markdown",
        "aliases": [
          "Markdown",
          "md"
        ],
        "extensions": [
          ".md"
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vibe-coding-sidebar",
          "title": "Vibe Coding - VICO",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "vibe-coding-sidebar": [
        {
          "type": "webview",
          "id": "vibe-coding-sidebar",
          "name": "Vibe Coding - VICO",
          "icon": "media/icon.svg",
          "contextualTitle": "Vibe Coding - VICO"
        }
      ]
    },
    "configuration": {
      "title": "Vibe Coding",
      "properties": {
        "vibeCoding.inline.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable inline code suggestions (ghost text) from Vibe Coding."
        },
        "vibeCoding.inline.debounceMs": {
          "type": "number",
          "default": 600,
          "minimum": 100,
          "maximum": 5000,
          "description": "Debounce time (ms) before requesting inline suggestions."
        }
      }
    },
    "activationEvents": [
      "onStartupFinished",
      "onView:vibe-coding-sidebar",
      "onLanguage:javascript",
      "onLanguage:typescript",
      "onLanguage:python",
      "onLanguage:go",
      "onLanguage:rust",
      "onLanguage:java",
      "onLanguage:cpp",
      "onLanguage:csharp",
      "onLanguage:php",
      "onLanguage:html",
      "onLanguage:css",
      "onLanguage:json",
      "onLanguage:markdown"
    ],
    "commands": [
      {
        "command": "vibe-coding.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vibe-coding.testSuggestion",
        "title": "Test Suggestion"
      },
      {
        "command": "vibe-coding.fetchSuggestions",
        "title": "Fetch Code Suggestions"
      },
      {
        "command": "vibe-coding.applyCode",
        "title": "Apply Code from Vibe Coding"
      },
      {
        "command": "vibe-coding.writeFile",
        "title": "Vibe Coding: Write File Vico"
      },
      {
        "command": "vibe-coding.findFiles",
        "title": "Vibe Coding: Find Files Vico"
      },
      {
        "command": "vibe-coding.updateWorkspaces",
        "title": "Vibe Coding: Update Workspaces"
      }
    ],
    "keybindings": [
      {
        "command": "vibe-coding.triggerCompletion",
        "key": "shift+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "vibe-coding.fetchSuggestions",
        "key": "ctrl+shift+.",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@pixiv/three-vrm": "^3.1.5",
    "crypto": "^1.0.1",
    "simple-audio": "^1.0.1",
    "three": "^0.170.0",
    "vico-logger": "^1.1.9"
  }
}
